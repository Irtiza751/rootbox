{
  "name": "@rootbox/base",
  "version": "1.0.0",
  "description": "base library of the rootbox",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf ./dist"
  },
  "devDependencies": {
    "@swc/core": "^1.6.13",
    "@swc/helpers": "^0.5.12",
    "@types/node": "^20.14.10",
    "tsup": "^8.1.1",
    "typescript": "^5.5.3"
  }
}
