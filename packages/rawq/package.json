{
  "name": "rawql",
  "version": "1.0.0",
  "description": "RawQL is light weight raw sql query builder tool",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf ./dist"
  },
  "keywords": ["sql", "query", "tool", "ts", "builder"],
  "author": "Muhammad Irtiza",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@swc/core": "^1.6.13",
    "@swc/helpers": "^0.5.12",
    "@types/node": "^20.14.10",
    "tsup": "^8.1.1",
    "typescript": "^5.5.4"
  }
}
